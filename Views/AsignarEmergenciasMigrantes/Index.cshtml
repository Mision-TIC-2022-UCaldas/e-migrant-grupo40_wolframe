@model IEnumerable<proyecto.Models.AsignarEmergenciasMigrante>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Detalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmergenciasMigrantes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntidadServicioEmergencia)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmergenciasMigrantes.IdEmergenciasMigrantes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EntidadServicioEmergencia.NombreEntidad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAsignarEmergenciasMigrante">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAsignarEmergenciasMigrante">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAsignarEmergenciasMigrante">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
