@model IEnumerable<proyecto.Models.SuplirNecesidad>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Detalle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EstadoServicios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Migrantes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Servicios)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Detalle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstadoServicios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Migrantes.Necesidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Servicios.Estado)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMigranteServicio">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMigranteServicio">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMigranteServicio">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
